<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true" android:id="@+id/state_on" android:drawable="@drawable/vd_clock_clock"/>
    <item android:id="@+id/state_off" android:drawable="@drawable/vd_clock_clock"/>
    <transition android:drawable="@drawable/avd_clock_clock" android:toId="@+id/state_on" android:fromId="@+id/state_off"/>
</animated-selector>
